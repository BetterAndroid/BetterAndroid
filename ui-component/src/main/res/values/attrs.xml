<resources>
    <declare-styleable name="SystemBarsView">
        <!-- Set the system bars insets type. -->
        <attr name="insetsType">
            <enum name="top" value="0" />
            <enum name="left" value="1" />
            <enum name="right" value="2" />
            <enum name="bottom" value="3" />
        </attr>
    </declare-styleable>
</resources>